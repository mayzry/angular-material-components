  "build:file-input": "ng build NgxMatFileInput",
  "publish:datetime-picker": "ng build @mayzry/angular-material-components-version_17 && cd dist/@mayzry/angular-material-components-version_17/angular-material-datetime-picker && npm publish --access public && cd .. && cd .. && cd ..",
